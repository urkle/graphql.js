    "test:coverage": "vitest run --coverage",
    "build": "vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f/graphql-client.git"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.31.1",
    "jest-mock-random": "^1.1.1",
    "msw": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.31.1",
    "vitest-plugin-set": "^1.0.0"
  },
  "keywords": [
    "graphql",
    "graphql-client",
    "jquery",
    "relay",
    "graphql-rails"
  ],
  "author": "Fatih Kadir AkÄ±n <fatihkadirakin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/f/graphql-client/issues"
  },
  "homepage": "https://github.com/f/graphql-client#readme",
  "jest": {
    "setupFiles": [
      "jest-plugin-set/setup"
    ]
