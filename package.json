{
  "name": "graphql.js",
  "version": "0.6.8",
  "description": "GraphQL Client for JavaScript",
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/graphql.js.umd.js",
  "module": "./dist/graphql.js.mjs",
  "exports": {
    ".": {
      "import": "./dist/graphql.js.mjs",
